<?xml version="1.0" encoding="utf-8"?>
<robot name="demo_robot">
  <!-- 底座 -->
  <link name="base_link">
    <innrtial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.001"/>
    </innrtial>
    <visual>
      <origin xyz="0.0 0.0 0.07" rpy="0 0 0.3"/>
      <geometry>
        <box size="0.3 0.3 0.1"/>
      </geometry>
      <material name="Cyan1">
        <color rgba="0 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.07" rpy="0 0 0.3"/>
      <geometry>
        <box size="0.3 0.3 0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- link1 -->
  <link name="link1">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.05"/>
      </geometry>
      <material name="Cyan2">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0.6"/>
      <geometry>
        <cylinder length="0.1" radius="0.05"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint1" type="revolute">
    <origin xyz="0.0 0.0 0.1" rpy="0 0 0.3"/>
    <parent link="base_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1"/>
  </joint>
</robot>